<div class="container">
    <div class="header clearfix">
        <h3 class="text-muted">Thông tin đơn hàng</h3>
    </div>
    <div class="table-responsive">
        <div class="form-group">
            <label>Mã đơn hàng:</label>

            <label>

            </label>
        </div>
        <div class="form-group">

            <label>Số tiền:</label>
            <label>
                <%= data.vnp_Amount %> VNĐ
            </label>
        </div>
        <div class="form-group">
            <label>Nội dung thanh toán:</label>
            <label>

            </label>
        </div>
        <div class="form-group">
            <label>Mã phản hồi (vnp_ResponseCode):</label>
            <label>
                <%= data.vnp_ResponseCode %>
            </label>
        </div>
        <div class="form-group">
            <label>Mã GD Tại VNPAY:</label>
            <label>
                <%= data.vnp_BankTranNo %>
            </label>
        </div>
        <div class="form-group">
            <label>Mã Ngân hàng:</label>
            <label>
                <%= data.vnp_BankCode %>
            </label>
        </div>
        <div class="form-group">
            <label>Thời gian thanh toán:</label>
            <label>
                <%= data.vnp_PayDate %>
            </label>
        </div>
        <div class="form-group">
            <label>Kết quả:</label>
            <% if(true){ %>
                <label>
                    Giao dịch thành công
                </label>
                <% } else{ %>
                    <label>
                        Giao dịch thất bại
                    </label>
                    <% } %>
                        <br>
                        <a href="/cart/checkout">
                            <button class="">Quay lại</button>
                        </a>
        </div>
    </div>
    <p>
        &nbsp;
    </p>
    <footer class="footer">
        <p>&copy; Quản lý Tiếng Anh 2020</p>
    </footer>
</div>