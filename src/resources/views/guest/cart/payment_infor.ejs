<div class="payment-container" style="text-align: center;
font-weight: 500;
font-size: 18px;
color: black;">
    <div class="header clearfix">
        <h3 class="text-muted">Thông tin đơn hàng</h3>
    </div>
    <div class="table-responsive" style="text-align: justify;
    margin: 0 auto;
    width: 22em;">
        <div class="form-group">
            <label>Kết quả:</label>
            <% if(data.vnp_ResponseCode==='00' ){ %>
                <label>
                    Giao dịch thành công
                </label>
                <% } else{ %>
                    <label>
                        Giao dịch thất bại
                    </label>
                    <% } %>

        </div>
        <div class="form-group">

            <label>Số tiền:</label>
            <label>
                <%= data.vnp_Amount %> VNĐ
            </label>
        </div>
        <div class="form-group">
            <label>Mã GD Tại VNPAY:</label>
            <label>
                <%= data.vnp_BankTranNo %>
            </label>
        </div>
        <div class="form-group">
            <label>Mã Ngân hàng:</label>
            <label>
                <%= data.vnp_BankCode %>
            </label>
        </div>
        <div class="form-group">
            <label>Thời gian thanh toán:</label>
            <label>
                <%= payDate.getDate() + '/' + (payDate.getMonth() + 1) + '/' + payDate.getFullYear() + ' ' + payDate.getHours() + ':' + (payDate.getMinutes() < 10 ? '0' + payDate.getMinutes() : payDate.getMinutes()) + ':' + payDate.getSeconds() %>
            </label>
        </div>
        <br>
    </div>
    <a href="/">
        <button class="btn btn-primary">Quay lại trang chủ</button>
    </a>
</div>